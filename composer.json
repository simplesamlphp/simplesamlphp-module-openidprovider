{
    "name": "simplesamlphp/simplesamlphp-module-openidprovider",
    "description": "A SimpleSAMLphp module adding support for the OpenID protocol as an Identity Provider.",
    "type": "simplesamlphp-module",
    "keywords": [ "openid", "provider" ],
    "homepage": "http://simplesamlphp.org",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Olav Morken",
            "email": "olav.morken@uninett.no"
        },
        {
            "name": "Andreas Ã…kre Solberg",
            "email": "andreas.solberg@uninett.no"
        }
    ],
    "include-path": ["./vendor/openid/php-openid/Auth"],
    "autoload": {
        "psr-4": {
            "SimpleSAML\\Module\\openidprovider\\": "lib/"
        },
        "classmap": ["vendor/openid/php-openid/Auth/"]
    },
    "extra": {
        "ssp-mixedcase-module-name": "openidProvider"
    },
    "require": {
        "simplesamlphp/composer-module-installer": "~1.0",
        "openid/php-openid": "~2.3.0"
    },
    "require-dev": {
        "simplesamlphp/simplesamlphp": "~1.17",
        "simplesamlphp/simplesamlphp-test-framework": "~0.0.10"
    }
}

